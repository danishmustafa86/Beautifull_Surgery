# API Configuration
REACT_APP_API_BASE_URL=http://localhost:8000
REACT_APP_API_VERSION=v1

# Environment
REACT_APP_ENV=development

# External Services
REACT_APP_GOOGLE_MAPS_API_KEY=your-google-maps-api-key
REACT_APP_MAPBOX_ACCESS_TOKEN=your-mapbox-access-token

# Analytics
REACT_APP_GOOGLE_ANALYTICS_ID=your-ga-tracking-id
REACT_APP_MIXPANEL_TOKEN=your-mixpanel-token

# Feature Flags
REACT_APP_ENABLE_ANALYTICS=false
REACT_APP_ENABLE_DEBUG=true
REACT_APP_ENABLE_MOCK_DATA=false

# App Configuration
REACT_APP_APP_NAME=Healthtech Directory
REACT_APP_APP_VERSION=1.0.0
REACT_APP_CONTACT_EMAIL=support@healthtech.com

# Map Configuration
REACT_APP_DEFAULT_MAP_CENTER_LAT=13.7563
REACT_APP_DEFAULT_MAP_CENTER_LNG=100.5018
REACT_APP_DEFAULT_MAP_ZOOM=12

# API Timeouts (in milliseconds)
REACT_APP_API_TIMEOUT=10000
REACT_APP_REQUEST_RETRY_COUNT=3

# Cache Configuration
REACT_APP_CACHE_DURATION=300000

# UI Configuration
REACT_APP_THEME_MODE=light
REACT_APP_PRIMARY_COLOR=#1976d2
REACT_APP_SECONDARY_COLOR=#9c27b0

# Error Reporting
REACT_APP_SENTRY_DSN=your-sentry-dsn-here
REACT_APP_ENABLE_ERROR_REPORTING=false

# Development Tools
REACT_APP_ENABLE_REDUX_DEVTOOLS=true
REACT_APP_ENABLE_CONSOLE_LOGS=true